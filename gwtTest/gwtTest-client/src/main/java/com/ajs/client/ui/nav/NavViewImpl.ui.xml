<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
             xmlns:g='urn:import:com.google.gwt.user.client.ui'
             xmlns:c='urn:import:com.google.gwt.user.cellview.client'>
    <ui:style>
        .border {
            border-top: solid #dcdcdc;
            border-right: solid #dcdcdc;
            border-bottom: solid #dcdcdc;
            border-left: solid #dcdcdc;
            border-image: none;
            border-width: 2px;
        }

        .floatRight {
            float: right;
        }

        .paddingContent {
            padding-top: 15px;
            padding-left: 25px;
            padding-right: 25px;
            padding-bottom: 25px;
        }

        .paddingImage {

            padding-right: 25px;
        }

        .paddingButton {
            padding-bottom: 60px;
            width: 1350px;
        }
            .bgblue {
                background: #6495ed
            }
            .bgwhite {
                background: #f5f5f5
            }
    </ui:style>

    <g:HTMLPanel>


        <!--<g:SimplePanel ui:field="employeeTablePanel"/>-->
        <g:SimplePanel ui:field="contentWrapperPanel">
            <g:FlowPanel height="755px" width="250px" addStyleNames='{style.border} {style.paddingContent}'>


            </g:FlowPanel>
        </g:SimplePanel>

    </g:HTMLPanel>
</ui:UiBinder>
