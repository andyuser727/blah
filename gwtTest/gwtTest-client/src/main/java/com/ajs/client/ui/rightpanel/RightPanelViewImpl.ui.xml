<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
             xmlns:g='urn:import:com.google.gwt.user.client.ui'
        >
    <ui:style>
        .border {
            border-top: solid #ffffff;
            border-right: solid #ffffff;
            border-bottom: solid #ffffff;
            border-left: solid #ffffff;
            border-image: none;
            border-width: 2px;
        }

        .shadow {
            box-shadow: 10px 10px 5px #888888;
            padding-right: 100px;
        }

        .floatRight {
            float: right;
        }

        .floatLeft {
            float: left;
        }

        .paddingContent {
            padding-right: 100px;
        }

        .paddingImage {

            padding-right: 25px;
        }

        .paddingButton {
            padding-bottom: 60px;
            width: 1350px;
        }

        .bgblue {
            background: #6495ed
        }

        .bgwhite {
            background: #f5f5f5
        }
        .bgGrey {
            background-color: #F7F1DC;
        }
    </ui:style>
            <g:FlowPanel height="465px" addStyleNames='{style.floatRight} {style.shadow} {style.border} {style.paddingContent} {style.bgGrey}'>
                <g:Label ui:field="rightPanelLabel"/>

            </g:FlowPanel>
</ui:UiBinder>
